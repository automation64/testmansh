#!/usr/bin/env bash

source bin/devtmsh-set || exit 1
source "$DEVTMSH_BIN/bashlib64.bash"

bl64_fs_rm_file "$DEVTMSH_BUILD_PROD"
bl64_fs_merge_files \
  '0755' \
  "$BL64_LIB_DEFAULT" \
  "$BL64_LIB_DEFAULT" \
  "$DEVTMSH_BUILD_PROD" \
  "$DEVTMSH_SRC/testmansh.header" \
  "$DEVTMSH_SRC/bashlib64.bash" \
  "$DEVTMSH_SRC/testmansh.bash"
